  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "sucrase": "^3.17.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.2"
  }
